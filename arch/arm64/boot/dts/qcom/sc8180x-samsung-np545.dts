&spmi_bus {
	pmic@2 {
		status = "disabled";
	};
	pmic@a {
		status = "disabled";
	};
};

&tlmm {
	i2c0_active: i2c0-active-state {
		pins = "gpio0", "gpio1";
		function = "qup0";

		bias-pull-up;
		drive-strength = <2>;
	};

	i2c1_active: i2c1-active-state {
		pins = "gpio114", "gpio115";
		function = "qup1";

		bias-pull-up;
		drive-strength = <2>;
	};

	i2c8_active: i2c8-active-state {
		pins = "gpio88", "gpio89";
		function = "qup8";

		bias-pull-up;
		drive-strength = <2>;
	};

	kb_int_active_state: kb-int-active-state {
		kb-int {
			pins = ""; // unknown
			function = "gpio";

			bias-pull-up;
			intput-enable;
		};
	};

	spi3_active: spi3-active-state {
		pins = "gpio144", "gpio145", "gpio146", "gpio147";
		function = "qup3";

		drive-strength = <2>;
		bias-disable;
	};

	tp_int_active_state: tp-int-active-state {
		tp-int {
			pins = ""; // unknown
			function = "gpio";

			bias-disable;
			input-enable;
		};
	};

	uart13_state: uart13-state {
		cts {
			pins = "gpio43";
			function = "qup13";
			bias-pull-down;
		};

		rts-tx {
			pins = "gpio44", "gpio45";
			function = "qup13";
			drive-strength = <2>;
			bias-disable;
		};

		rx {
			pins = "gpio46";
			function = "qup13";
			bias-pull-up;
		};
	};
};

&uart13 {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&uart13_state>;
};

&ufs_mem_hc {
	status = "okay";

	reset-gpios = <&tlmm 190 GPIO_ACTIVE_LOW>;

	vcc-supply = <&vreg_l10e_2p9>;
	vcc-max-microamp = <155000>;

	vccq2-supply = <&vreg_l7e_1p8>;
	vccq2-max-microamp = <425000>;
