&dispcc {
	status = "okay";
};

&gpu {
	status = "okay";

	zap-shader {
		memory-region = <&gfxsuc_mem>;
		firmware-name = "qcom/samsung/np545/qcdxkmsuc8180.mbn";
	};
};

&i2c0 {
	status = "okay";
	clock-frequency = <400000>;

	pinctrl-names = "default";
	pinctrl-0 = <&i2c0_active>;

	touchpad@40 {
		compatible = "hid-over-i2c";
		reg = <0x40>;
		hid-descr-addr = <0x1>;

		interrupts-extended = <&tlmm 0 IRQ_TYPE_LEVEL_LOW>; // unknown

		pinctrl-names = "default";
		pinctrl-0 = <&tp_int_active_state>;

		vdd-supply = <&vreg_l11c_3p3>;
	};
};

&i2c1 {
	status = "okay";
	clock-frequency = <400000>;

	pinctrl-names = "default";
	pinctrl-0 = <&i2c1_active>;
};

&i2c8 {
	status = "okay";
