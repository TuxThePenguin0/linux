	pinctrl-names = "default";
	pinctrl-0 = <&i2c8_active>;

	keyboard@5 {
		compatible = "hid-over-i2c";
		reg = <0x05>;
		hid-descr-addr = <0x1>;

		interrupts-extended = <&tlmm 0 IRQ_TYPE_LEVEL_LOW>; // unknown

		pinctrl-names = "default";
		pinctrl-0 = <&kb_int_active_state>;
	};
};

